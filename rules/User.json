{
  "entityName": "User",
  "properties": [
    {
      "name": "Username",
      "type": "string",
      "rules": [
        { "type": "NotEmpty", "message": "Username is required" },
        {
          "type": "Length",
          "min": 3,
          "max": 20,
          "message": "Username must be between 3 and 20 chars"
        }
      ]
    },
    {
      "name": "Email",
      "type": "string",
      "rules": [{ "type": "NotEmpty" }, { "type": "EmailAddress" }]
    },
    {
      "name": "Age",
      "type": "number",
      "rules": [
        { "type": "GreaterThan", "value": 18 },
        { "type": "LessThan", "value": 100 }
      ]
    }
  ]
}
